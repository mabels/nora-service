{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": {
        "anyOf": [
            {
                "$ref": "#/definitions/Partial<State>"
            },
            {
                "$ref": "#/definitions/Partial<OpenCloseState>"
            },
            {
                "$ref": "#/definitions/Partial<OnOffState>"
            },
            {
                "$ref": "#/definitions/Partial<LockUnlockState>"
            },
            {
                "$ref": "#/definitions/Partial<Thermostat>"
            },
            {
                "$ref": "#/definitions/Partial<BrightnessState&OnOffState>"
            },
            {
                "$ref": "#/definitions/Partial<ColorState&BrightnessState&OnOffState>"
            },
            {
                "$ref": "#/definitions/Partial<VolumeState&OnOffState>"
            },
            {
                "$ref": "#/definitions/Partial<Partial<Thermostat>>"
            }
        ]
    },
    "definitions": {
        "Partial<BrightnessState&OnOffState>": {
            "additionalProperties": false,
            "properties": {
                "brightness": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                },
                "on": {
                    "type": "boolean"
                },
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Partial<ColorState&BrightnessState&OnOffState>": {
            "additionalProperties": false,
            "properties": {
                "brightness": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                },
                "color": {
                    "additionalProperties": false,
                    "properties": {
                        "HSV": {
                            "additionalProperties": false,
                            "properties": {
                                "h": {
                                    "maximum": 360,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "s": {
                                    "maximum": 1,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "v": {
                                    "maximum": 1,
                                    "minimum": 0,
                                    "type": "number"
                                }
                            },
                            "required": [
                                "h",
                                "s",
                                "v"
                            ],
                            "type": "object"
                        },
                        "RGB": {
                            "additionalProperties": false,
                            "properties": {
                                "b": {
                                    "maximum": 255,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "g": {
                                    "maximum": 255,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "r": {
                                    "maximum": 255,
                                    "minimum": 0,
                                    "type": "number"
                                }
                            },
                            "required": [
                                "b",
                                "g",
                                "r"
                            ],
                            "type": "object"
                        },
                        "spectrumHsv": {
                            "additionalProperties": false,
                            "properties": {
                                "hue": {
                                    "maximum": 360,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "saturation": {
                                    "maximum": 1,
                                    "minimum": 0,
                                    "type": "number"
                                },
                                "value": {
                                    "maximum": 1,
                                    "minimum": 0,
                                    "type": "number"
                                }
                            },
                            "required": [
                                "hue",
                                "saturation",
                                "value"
                            ],
                            "type": "object"
                        },
                        "temperature": {
                            "maximum": 6000,
                            "minimum": 2000,
                            "type": "number"
                        }
                    },
                    "type": "object"
                },
                "on": {
                    "type": "boolean"
                },
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Partial<LockUnlockState>": {
            "additionalProperties": false,
            "properties": {
                "isJammed": {
                    "type": "boolean"
                },
                "isLocked": {
                    "type": "boolean"
                },
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Partial<OnOffState>": {
            "additionalProperties": false,
            "properties": {
                "on": {
                    "type": "boolean"
                },
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Partial<OpenCloseState>": {
            "additionalProperties": false,
            "properties": {
                "online": {
                    "type": "boolean"
                },
                "openPercent": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                }
            },
            "type": "object"
        },
        "Partial<Partial<Thermostat>>": {
            "additionalProperties": false,
            "properties": {
                "online": {
                    "type": "boolean"
                },
                "thermostatHumidityAmbient": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                },
                "thermostatMode": {
                    "enum": [
                        "auto",
                        "cool",
                        "dry",
                        "eco",
                        "fan-only",
                        "heat",
                        "heatcool",
                        "off",
                        "on",
                        "purifier"
                    ],
                    "type": "string"
                },
                "thermostatTemperatureAmbient": {
                    "type": "number"
                },
                "thermostatTemperatureSetpoint": {
                    "type": "number"
                },
                "thermostatTemperatureSetpointHigh": {
                    "type": "number"
                },
                "thermostatTemperatureSetpointLow": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "Partial<State>": {
            "additionalProperties": false,
            "properties": {
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Partial<Thermostat>": {
            "additionalProperties": false,
            "properties": {
                "online": {
                    "type": "boolean"
                },
                "thermostatHumidityAmbient": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                },
                "thermostatMode": {
                    "enum": [
                        "auto",
                        "cool",
                        "dry",
                        "eco",
                        "fan-only",
                        "heat",
                        "heatcool",
                        "off",
                        "on",
                        "purifier"
                    ],
                    "type": "string"
                },
                "thermostatTemperatureAmbient": {
                    "type": "number"
                },
                "thermostatTemperatureSetpoint": {
                    "type": "number"
                },
                "thermostatTemperatureSetpointHigh": {
                    "type": "number"
                },
                "thermostatTemperatureSetpointLow": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "Partial<VolumeState&OnOffState>": {
            "additionalProperties": false,
            "properties": {
                "currentVolume": {
                    "maximum": 100,
                    "minimum": 0,
                    "type": "number"
                },
                "isMuted": {
                    "type": "boolean"
                },
                "on": {
                    "type": "boolean"
                },
                "online": {
                    "type": "boolean"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

